{
    "presets": [
        "react",
        "es2015",
        ["env", {
            "targets": {
                "browsers": "last 2 versions"
            },
            "modules": false
        }]
    ],
    "plugins": ["transform-object-rest-spread", "babel-plugin-transform-class-properties"],
    "env": {
        "server": {
            "plugins": ["transform-es2015-modules-commonjs"]
        }
    }
}
